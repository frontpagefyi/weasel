{
  "name": "vaul",
  "version": "1.1.2",
  "description": "Drawer component for React.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "style.css"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "pnpm type-check && bunchee && pnpm copy-assets",
    "copy-assets": "cp -r ./src/style.css ./style.css",
    "dev": "bunchee --watch",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal"
  ],
  "author": "Emil Kowalski <e@emilkowal.ski>",
  "license": "MIT",
  "homepage": "https://vaul.emilkowal.ski/",
  "repository": {
    "type": "git",
    "url": "https://github.com/emilkowalski/vaul.git"
  },
  "bugs": {
    "url": "https://github.com/emilkowalski/vaul/issues"
  },
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "^5.1.5",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1"
  }
}
