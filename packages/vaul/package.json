{
  "name": "vaul",
  "version": "1.1.2",
  "description": "Drawer component for React.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "style.css"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "pnpm type-check && bunchee && pnpm copy-assets",
    "copy-assets": "ncp ./src/style.css ./dist/style.css",
    "dev": "bunchee --watch",
    "lint": "eslint --flag unstable_native_nodejs_ts_config src/",
    "lint:fix": "pnpm lint --fix"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal"
  ],
  "author": "Emil Kowalski <e@emilkowal.ski>",
  "license": "MIT",
  "homepage": "https://vaul.emilkowal.ski/",
  "repository": {
    "type": "git",
    "url": "https://github.com/emilkowalski/vaul.git"
  },
  "bugs": {
    "url": "https://github.com/emilkowalski/vaul/issues"
  },
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@repo/eslint-config": "workspace:^",
    "@repo/typescript-config": "workspace:^",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "^5.1.5",
    "eslint": "catalog:",
    "globals": "^16.5.0",
    "ncp": "^2.0.0",
    "prettier": "^2.5.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc",
    "react-dom": "^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1"
  }
}
